{
  "name": "leo-landing-page-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next lint && next build && next export && next-export-optimize-images",
    "start": "npm run build && npx serve out",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postbuild": "next-sitemap",
    "update-ui-library": "( cd ../leo-landing-page-ui-library ; npm run build ; npm pack ) && cp ../leo-landing-page-ui-library/leo-landing-page-ui-library-*.tgz ./lib && rm ../leo-landing-page-ui-library/leo-landing-page-ui-library-*.tgz && rm -rvf .next && npm install ui-library --force"
  },
  "dependencies": {
    "next": "12.2.3",
    "next-seo": "^5.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ui-library": "file:lib/leo-landing-page-ui-library-0.0.0.tgz"
  },
  "devDependencies": {
    "@types/node": "18.6.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.2.3",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "next-export-optimize-images": "^1.5.3",
    "typescript": "4.7.4"
  }
}
